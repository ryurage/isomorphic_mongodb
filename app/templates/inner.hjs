<!DOCTYPE html>
<html>
    <head>
        <title>{{ title }}</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
        <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
        <script src="/js/main.js" type="text/javascript"></script>
        <script src="/js/brandonminton_cal/public_cal.js" type="text/javascript"></script>
    </head>
    <body>
        <div class="container">
            
            <!-- navigation -->
            <nav>
                <div class="content">
                    <a href="/" class="logo">
                        <!--<img src="/images/logo.png" alt="FastDelivery">-->
                    </a>

                    <br class="clear" />
                </div>
            </nav>
            <div class="cover-menu">
                <ul class="menu">
                    <li tab="tab1" class="current"><a href="/">Home</a></li>
                    <li tab="tab2"><a href="/blog">Blog</a></li>
                    <li tab="tab3"><a href="/services">Services</a></li>
                    <li tab="tab4"><a href="/careers">Careers</a></li>
                    <li tab="tab5" class="last"><a href="/contacts">Contacts</a></li>
                </ul>
            </div>
            
            <!-- inner page -->
            <div class="inner">
                <div class="teaser">
                    <div class="content">
                        <section>
                            <img src="{{picture}}" alt="" />
                            <h1>{{title}}</h1>
                            <p>{{{text}}}</p>
                        </section>
                    </div>
                </div>
            </div>
            
            <!-- footer -->
            <div class="footer">
                <div class="content">
                    fast delivery &copy; 2013
                </div>
            </div>
            
		</div>
	</body>
</html>